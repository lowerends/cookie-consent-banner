{
    "name": "lowerends/cookie-consent-banner",
    "description": "Add a configurable cookie consent banner to the website.",
    "type": "craft-plugin",
    "version": "3.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "cookie consent banner"
    ],
    "support": {
        "docs": "https://github.com/lowerends/cookie-consent-banner/blob/master/README.md",
        "issues": "https://github.com/lowerends/cookie-consent-banner/issues"
    },
    "license": "MIT",
    "authors": [
		{
			"name": "Mark @ A Digital",
			"homepage": "https://adigital.agency"
		},
		{
			"name": "Marko GajiÄ‡",
			"homepage": "https://developion.com"
		}
    ],
    "require": {
        "craftcms/cms": "^5.0"
    },
    "autoload": {
        "psr-4": {
          "lowerends\\cookieconsentbanner\\": "src/"
        }
    },
    "extra": {
        "name": "Cookie Consent Banner",
        "handle": "cookie-consent-banner",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/lowerends/cookie-consent-banner/blob/master/CHANGELOG.md",
        "class": "lowerends\\cookieconsentbanner\\CookieConsentBanner"
    },
    "config": {
        "platform": {
          "php": "8.0.2"
        },
        "allow-plugins": {
          "yiisoft/yii2-composer": true,
          "craftcms/plugin-installer": true
        }
    }
}
